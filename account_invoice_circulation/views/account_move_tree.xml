<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_in_invoice_tree" model="ir.ui.view">
        <field name="name">account.out.invoice.tree.circulation</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_in_invoice_tree" />
        <field name="arch" type="xml">

            <field name="move_type" position="before">
                <field name="current_approver_id" optional="hide" />
                <field
                    name="account_invoice_circulation_id"
                    groups="account.group_account_manager"
                    optional="hide"
                />
            </field>

        </field>
    </record>
</odoo>
