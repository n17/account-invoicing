<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="move_line_tree_commission" model="ir.ui.view">
        <field name="name">account.move.line.tree.commission</field>
        <field name="model">account.move.line</field>
        <field name="priority" eval="99" />
        <field name="arch" type="xml">

            <tree string="Invoice lines" delete="false">
                <field name="date" />
                <field name="move_name" />
                <field name="product_id" />
                <field name="name" />
                <field name="margin" string="Margin" sum="Total margin" />
                <field
                    name="purchase_price"
                    string="Payment"
                    sum="Total payment"
                />
                <field
                    name="price_subtotal"
                    string="Tax Excluded"
                    sum="Total"
                />
                <field name="currency_id" invisible="1" />
                <field name="company_currency_id" invisible="1" />
                <field name="parent_state" />
                <field name="commission_paid" />
            </tree>

        </field>
    </record>
</odoo>
